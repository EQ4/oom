/* Mixer Audio In Style*/
QFrame#MixerAudioInStrip { 
	border: 1px solid #29292a;
	/*background-color: #5f855f;*/
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #749f71, stop:0.5 #4d6a4c);
	border-radius: 7px;
}
