/*
 *    OOMidi application style sheet
 */

QWidget#MRConfigBase, QWidget#MITTransposeBase 
{
	background-color: #595966;
}
QWidget#dockMixerCenter 
{
	background-color: #595966;
}
QWidget#transport 
{
	background-color: #595966;
}
QTableView#trackHeaders 
{
	background-color: #595966;
	border: none;
	padding: 0px;
}

QHeaderView#trackHeaders 
{
	text-align: left;
	border: none;
	background-color: #595966;
}

 QWidget#WaveView
 {
 	background-color: black;
 }
 QCanvas#ctrlCanvas
 {
 }
 QScrollArea#scrollArea
