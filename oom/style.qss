/*Trackview editor styles*/
TrackViewEditor QListView, TrackViewEditor QTextField, QTableView
{
	background-color: #d6d2d6;
	border: 1px solid #29292a;
	border-radius: 6px;
	padding: 8px;
}

/*MidiTrackInfo styles*/

QLabel#patchLabel
{
	 border-radius: 8px;
     border-image: url(:/images/frame.png) 4;
     border-width: 3;
	 border-color: #484848;
	 background-color: #010f14;
	 color: #03c0e2;
	 padding: 0px 8px;
}

MidiTrackInfo QTableView, QTableView#mapTable
{
	background-color: #d8dbe8;
	border: 2px solid #29292a;
	border-radius: 0px;
	padding: 0px;
	color: black;
	font-size: 14px;
	alternate-background-color: #bec0cf;
}
MidiTrackInfo QTableView::focus, QTableView#mapTable::focus
{
	border: 2px solid #f3ce69;
}

MidiTrackInfo QTableView QHeaderView::section 
{
	text-align: left;
	border: 0px solid black;
	border-radius: 0px;
	padding: 5px;
	/*background-color: #936b9c;*/
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #727180, stop:0.1 #727180, stop:0.3 #4a4d5a, stop:1 #4a4d5a);
	color: #d6dfd6;
	font-family: "fixed-width";
	font-weight: bold;
	font-size: 11px;
}

MidiTrackInfo QTableView::item 
{
	padding-top: 8px;
	padding-bottom: 8px;
}

MidiTrackInfo QTableView::item:selected, QTableView#mapTable::item:selected 
{
	background-color: #3d3e44;
	border: none;
}

MidiTrackInfo QTableView::item:selected:active 
{
	border: none;
}

MidiTrackInfo QTableView::item:selected:!active 
{
	border: none;
}

/* Patch tree styles */
MidiTrackInfo QWidget#patchSeqContents
{
	background-color: #595966;
	color: #d0d4d0;
}

MidiTrackInfo QTreeView/*{{{*/
{
	background-color: #d8dbe8;
	border: 2px solid #29292a;
	border-radius: 0px;
	padding: 0px;
	color: #303033;
	font-size: 14px;
